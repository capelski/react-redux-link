{
    "name": "react-redux-link",
    "version": "1.0.2",
    "description": "Utility to type check component properties when connecting a react component to the redux store",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc -p ./tsconfig.json",
        "prettier": "prettier --write \"{src,test}/**/*.{ts,tsx}\"",
        "test": "tsc -p test/tsconfig.json",
        "watch": "tsc -p ./tsconfig.json -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/capelski/react-redux-link.git"
    },
    "keywords": [
        "react-redux",
        "redux",
        "connect"
    ],
    "author": "Carles Capellas <capellas.carles@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/capelski/react-redux-link/issues"
    },
    "homepage": "https://github.com/capelski/react-redux-link#readme",
    "devDependencies": {
        "@types/react-redux": "*",
        "prettier": "^2.1.2",
        "react": "^17.0.1",
        "typescript": "^4.0.5"
    },
    "dependencies": {
        "redux": "*",
        "react-redux": "*"
    }
}
